extends layout

block content
  div(class='header')
    h1 Omega
    p The new Alpha, right now in Beta 
  
  
  div(class='message')
    h3 #{title} Login
    
  div(class="main")  
    p(class="sign" align="center") Log In
    form(class="form1" action='#', method='POST') 
      input(class="un" type='text' align="center" placeholder="Username")
      input(class="pass" type="password" align="center" placeholder="Password")
      a(class="submit" align="center") Log In
  div(class="back")
    a( href="/" ) Back To Homepage
      
